[global]
name=zns-fio
directory=/mnt/f2fs
ioengine=io_uring
fixedbufs=1
registerfiles=1
sqthread_poll=1
iodepth=32
rw=write
bs=4K
runtime=30s
numjobs=1
thread=1
ramp_time=10s 
ioscheduler=mq-deadline
group_reporting

[hot-file]
size=20G
write_hint=short

[warm-file]
size=20G
write_hint=medium

[cold-file]
size=20G
write_hint=extreme
