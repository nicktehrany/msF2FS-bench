[global]
name=zns-fio
directory=/mnt/f2fs
size=5G
bs=4K
ioscheduler=mq-deadline
group_reporting
thread=1

[write]
stonewall
numjobs=7
ioengine=io_uring
ioengine=io_uring
fixedbufs=1
registerfiles=1
sqthread_poll=1
iodepth=32
direct=1
rw=read
filename_format=gc.$jobnum.0
percentile_list=50:75:90:95:99:99.9:99.99:99.999:100
