[global]
name=zns-fio
directory=/mnt/f2fs
size=5G
bs=4K
ioscheduler=mq-deadline
group_reporting
thread=1

[fill-prep]
ioengine=io_uring 
fixedbufs=1
registerfiles=1
sqthread_poll=1
direct=1
iodepth=128
numjobs=8
filename_format=gc.$jobnum.$filenum
fsync=1
write_hint=short
rw=write
write_bw_log=${DIR}/fill_bw_log
write_lat_log=${DIR}/fill_lat_log
write_iops_log=${DIR}/fill_iops_log
log_avg_msec=1000
