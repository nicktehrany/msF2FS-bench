[global]
name=zns-fio
directory=/mnt/f2fs
size=520G
bs=4K
ioscheduler=mq-deadline
group_reporting
thread=1

[overwrite]
stonewall
loops=8
numjobs=3
ioengine=io_uring 
fixedbufs=1
registerfiles=1
sqthread_poll=1
direct=1
iodepth=128
write_hint=short
rw=write
fsync=32
filename_format=file.$jobnum.0
overwrite=1
